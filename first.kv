<LoadDialog>:
    id: loadd
    BoxLayout:
        size: root.size
        pos: root.pos
        orientation: "vertical"
        spacing: 10
        padding: 10
        BoxLayout:
            canvas.before:
                Color:
                    rgba: 0,0,0,.4
                Rectangle:
                    size: self.size
                    pos: self.pos
            FileChooserListView:
                id: filechooser
                bar_width: 5
                path: "datatest_gbesar"
                on_selection: loadd.selected(filechooser.selection)
        BoxLayout:
            size_hint_y: None
            height: 50
            Label:
                text: "Preview"
                font_name: "DejaVuSans"
        Image:
            id: ban
            opacity: 0
            source: ""
        BoxLayout:
            size_hint_y: None
            height: 20
        BoxLayout:
            size_hint_y: None
            height: 75
            spacing: 40
            Button:
                on_release: root.cancel()
                text: "BATAL"
                color: 1,1,1,1
                background_color: 0, 0, 0, 0
                font_name: "Roboto-Bold"
                # font_size: 0.31 * self.height
                canvas.before:
                    Color:
                        rgb: .807, .807, .807
                    Rectangle:
                        size: self.size
                        pos: self.pos
            Button:
                on_release: app.load(filechooser.selection)
                text: "PILIH"
                color: 1,1,1,1
                background_color: 0, 0, 0, 0
                font_name: "Roboto-Bold"
                # font_size: 0.31 * self.height
                canvas.before:
                    Color:
                        rgb: 1,0,0
                    Rectangle:
                        size: self.size
                        pos: self.pos
<Kamera>:
    id: kame
    BoxLayout:
        size: root.size
        pos: root.pos
        orientation: 'vertical'
        Camera:
            id: camera
            resolution: (640, 480)
            play: True
            canvas.before:
                PushMatrix
                Rotate:
                    angle: -90
                    origin: self.center
            canvas.after:
                PopMatrix
            Image:
                id: pembatas
                source: "pembatas.png"
                center_x: self.parent.center_x
                center_y: self.parent.center_y
                size: 450, 600
                allow_stretch: True
                show: False
                canvas.before:
                    PushMatrix
                    Rotate:
                        angle: -90
                        origin: self.center
                canvas.after:
                    PopMatrix
        BoxLayout:
            size_hint_y: None
            height: 75
            spacing: 20
            Button:
                on_release: root.cancel()
                text: "BATAL"
                color: 1,1,1,1
                background_color: 0, 0, 0, 0
                font_name: "Roboto-Bold"
                # font_size: 0.31 * self.height
                canvas.before:
                    Color:
                        rgb: .807, .807, .807
                    Rectangle:
                        size: self.size
                        pos: self.pos
            Button:
                on_press: kame.capture()
                on_release: app.foto()
                text: "CAPTURE"
                color: 1,1,1,1
                background_color: 0, 0, 0, 0
                font_name: "Roboto-Bold"
                # font_size: 0.31 * self.height
                canvas.before:
                    Color:
                        rgb: 1,0,0
                    Rectangle:
                        size: self.size
                        pos: self.pos
